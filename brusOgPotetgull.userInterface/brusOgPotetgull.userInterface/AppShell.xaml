<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="brusOgPotetgull.userInterface.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:brusOgPotetgull.userInterface.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="brusOgPotetgull.userInterface"
    >
    <TabBar>
        <ShellContent
        Title="Create your Airport"
        ContentTemplate="{DataTemplate views:MainPage}"
        Route="MainPage" />
        
        <ShellContent
            Title="Airport Controlpanel"
            ContentTemplate="{DataTemplate views:AirportInterface}"
            Route="AirportInterface" />
    </TabBar>

</Shell>
